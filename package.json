{
    "name": "ecospense",
    "version": "0.1.0",
    "private": true,
    "description": "A JAM stack PWA, expense tracker web app built with React, TypeScript, Next.js, Prisma and MongoDB.",
    "keywords": [
        "finance",
        "expense",
        "budget",
        "PWA",
        "React",
        "TypeScript",
        "Next.js",
        "next-auth"
    ],
    "scripts": {
        "dev": "NODE_ENV=development next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "export": "next build && next export",
        "vercel-build": "prisma generate && next build",
        "migrate:deploy": "prisma migrate deploy",
        "migrate:prisma": "prisma migrate dev",
        "dbpush:prisma": "prisma db push",
        "generate:prisma": "prisma generate",
        "introspect:prisma": "prisma introspect",
        "studio:prisma": "prisma studio",
        "format:prisma": "prisma format",
        "seed:prisma": "prisma db seed"
    },
    "engines": {
        "node": "^16"
    },
    "prisma": {
