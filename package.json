{
    "name": "ecospense",
    "version": "0.1.0",
    "private": true,
    "description": "A JAM stack PWA, expense tracker web app built with React, TypeScript, Next.js, Prisma, Apollo GraphQL, Nexus Schema and MongoDB.",
    "keywords": [
        "finance",
        "expense",
        "budget",
        "PWA",
        "React",
        "TypeScript",
        "Next.js",
        "next-auth"
    ],
    "scripts": {
        "dev": "NODE_ENV=development next dev",
        "build": "next build",
        "start": "NODE_ENV=production next start",
        "lint": "next lint",
        "export": "next build && next export",
        "vercel-build": "prisma generate && next build",
        "migrate:deploy": "prisma migrate deploy",
        "migrate:prisma": "prisma migrate dev",
        "dbpush:prisma": "prisma db push",
        "generate:prisma": "prisma generate",
        "introspect:prisma": "prisma introspect",
        "studio:prisma": "prisma studio",
        "format:prisma": "prisma format",
        "seed:prisma": "prisma db seed"
    },
    "engines": {
        "node": "^16",
        "yarn": "^1.22"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "dependencies": {
        "@apollo/client": "^3.7.10",
        "@apollo/server": "^4.5.0",
        "@as-integrations/next": "^1.2.0",
        "@dicebear/collection": "^5.3.4",
        "@dicebear/core": "^5.3.4",
        "@next-auth/mongodb-adapter": "^1.1.1",
        "@next-auth/prisma-adapter": "^1.0.5",
        "@prisma/client": "^4.11.0",
        "@splidejs/react-splide": "^0.7.12",
        "apexcharts": "^3.37.2",
        "currency-converter-lt": "^2.0.0-beta.0",
        "framer-motion": "^10.9.1",
        "graphql": "^16.6.0",
        "ml-matrix": "^6.10.4",
        "mongodb": "^5.1.0",
        "next": "13.2.4",
        "next-auth": "^4.20.1",
        "next-seo": "^5.15.0",
        "nextjs-progressbar": "^0.0.16",
        "nexus": "^1.3.0",
        "nodemailer": "^6.9.1",
        "otp-generator": "^4.0.1",
        "react": "18.2.0",
        "react-apexcharts": "^1.4.0",
        "react-countdown": "^2.3.4",
        "react-dom": "18.2.0",
        "react-ionicons": "^4.2.0",
        "react-simple-pull-to-refresh": "^1.3.3",
        "react-use-is-online": "^1.3.0",
        "styled-components": "^5.3.9",
        "winston": "^3.8.2",
        "xss": "^1.0.14"
    },
    "devDependencies": {
        "@types/next-auth": "^3.15.0",
        "@types/node": "18.15.9",
        "@types/nodemailer": "^6.4.7",
        "@types/otp-generator": "^4.0.0",
        "@types/react": "^18.0.29",
        "@types/react-dom": "18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "eslint": "8.36.0",
        "eslint-config-next": "13.2.4",
        "eslint-plugin-react": "^7.32.2",
        "prettier": "^2.8.7",
        "prisma": "^4.11.0",
        "pwa-asset-generator": "^6.2.1",
        "ts-node": "^10.9.1",
        "typescript": "5.0.2"
    }
